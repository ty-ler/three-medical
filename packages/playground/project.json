{
  "root": "packages/playground",
  "sourceRoot": "packages/playground/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nxext/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "frameworkConfigFile": "@nxext/svelte/plugins/vite",
        "outputPath": "dist/packages/playground",
        "assets": [
          {
            "glob": "/*",
            "input": "./public/**",
            "output": "./"
          }
        ],
        "tsConfig": "packages/playground/tsconfig.app.json"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "frameworkConfigFile": "@nxext/svelte/plugins/vite",
        "outputPath": "dist/packages/playground",
        "assets": [
          {
            "glob": "/*",
            "input": "./public/**",
            "output": "./"
          }
        ],
        "tsConfig": "packages/playground/tsconfig.app.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/playground/**/*.{ts,svelte,spec.ts}"]
      }
    },
    "check": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "packages/playground"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/playground"],
      "options": {
        "jestConfig": "packages/playground/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
